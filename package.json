{
  "name": "educational-api",
  "version": "1.0.0",
  "description": "- The project consists on the api of a educational platform using nodejs and fastify.",
  "main": "index.js",
  "scripts": {
    "build": "tsup src --out-dir build",
    "start:dev": "tsx watch src/main.ts",
    "start": "node build/main.js",
    "test": "vitest run",
    "test:e2e": "vitest run --config ./vite.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vite.config.e2e.mts",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint",
    "start:prisma": "yarn prisma generate && yarn prisma migrate deploy",
    "start:render": "yarn start:prisma && yarn build && yarn start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.106",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "7.0.0",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.8.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.2",
    "fastify": "^4.25.2",
    "zod": "^3.22.4"
  }
}